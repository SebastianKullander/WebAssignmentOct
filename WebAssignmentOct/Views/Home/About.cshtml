@model List<Table>
@{
    ViewBag.Title = "About";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<body>
    <div class="container">
        <div class="jumbotron">
            <h1>Om mig</h1>
            <p1> 
                Just nu pluggar jag ett fjärde år på Östra Gymnasiet.
            </p1>

            <h2>
                Utbildningar:
            </h2>
            
            <ul>
                @foreach (var item in Model)
                {
                    <li>@item.education | @item.period</li>
                }
            </ul>

            <hr class="m-y-2">
            <p class="lead">
                <a class="btn btn-primary" href="/" role="button">Home</a>
                <a class="btn btn-primary" href="/Home/About" role="button">Om mig</a>
                <a class="btn btn-primary" href="/Home/Contact" role="button">Kontakt</a>
            </p>


        </div>

    </div>
</body>
